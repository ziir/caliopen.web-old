{
  "name": "caliopen.web",
  "version": "0.0.0",
  "description": "CaliOpen Web Server using NodeJS/Express",
  "repository": {
    "type": "git",
    "url": "git@gitlab.corp.gandi.net:tim/caliopen.web.git"
  },
  "author": "Gandi",
  "private": false,
  "scripts": {
    "start": "DEBUG=caliopen.web:* NODE_ENV=development node ./bin/start",
    "start:watch": "DEBUG=caliopen.web:* NODE_ENV=development ./node_modules/nodemon/bin/nodemon.js ./bin/start",
    "start:production": "NODE_ENV=production ./node_modules/.bin/pm2 start ./bin/start --name caliopen-web",
    "test": "node ./bin/test"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "nodemon": "^1.4.1",
    "nunjucks": "^1.3.4",
    "pm2": "^0.14.7"
  }
}
